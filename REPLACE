#include <stdio.h>
int main(void)
{
int a[5]={1,2,3,4,5},number,b,find;
printf("Enter the number to be searched:");
scanf("%d",&number);
printf("Enter the number to be updated:");
scanf("%d",&b);
for(int i=0;i<5;i++)
{
  if(number == a[i])
  {
    a[i]=b;
    find=1;
    break;
  }
}
if(find==1)
{
  printf("The Updated array is: ");
  for(int i=0;i<5;i++)
  {
    printf("%d",a[i]);
  }
}
else{

printf("The element is not found");

  return 0;
}
